<body class="gray-bg" style="background-color: #fff;">
	<div class="zhezhao" id="zhezhao" style="display:none;"></div>
	<div class="zhezhao_title" id="zhezhao_title" style="display:none;">
		<div class="sk-spinner sk-spinner-wave" style="width: 200px;">
			<h2><b><span style="color: #fff;">创建中......</span></b></h2>
		</div>
		<div class="sk-spinner sk-spinner-wave">
	        <div class="sk-rect1"></div>
	        <div class="sk-rect2"></div>
	        <div class="sk-rect3"></div>
	        <div class="sk-rect4"></div>
	        <div class="sk-rect5"></div>
	    </div>
	</div>
	
	<div class="wrapper wrapper-content animated fadeInRight" style="height:100%;">
    	<div class="row">
    		<div class="col-sm-4">
				<div class="input-group">
					<span class="input-group-addon"> 任务名称： </span>
					<div class="chaxundiv_2 selectDiv" id="">
						<input id="renwu_name" placeholder="请填写任务名称" type="text" style="width: 100%;height: 33px;">
					</div>
				</div>
			</div>
			
			<div class="col-sm-4">
				<div class="input-group">
					<span class="input-group-addon">清单年份</span>
					<div class="chaxundiv_2 selectDiv" id="data_1">
						<div class="input-group date" style="width:100%;">
							<span class="input-group-addon"><i class="fa fa-calendar"></i></span>
							<input type="text" id="qdnf" style="width: 100%;height: 33px;" />
						</div>
					</div>
				</div>
			</div>
			<div class="col-sm-4">
				<div class="input-group">
					<span class="input-group-addon">任务截止日期</span>
					<div class="chaxundiv_2 selectDiv" id="data_2">
						<div class="input-group date" style="width:100%;">
							<span class="input-group-addon"><i class="fa fa-calendar"></i></span>
							<input type="text" id="rwjzrq" style="width: 100%;height: 33px;" />
						</div>
					</div>
				</div>
			</div>
			<!-- <div class="col-sm-3">
				<button class="btn btn-outline btn-lG" onclick="save()" style="float:right;margin-right:10px;">
					<i class="fa fa-plus-square"></i> 新建任务
				</button>
			</div> -->
    	</div>
    	<br>
    	<div class="row">
    		<div class="col-sm-12">
				<div class="example-wrap">
					<div class="example">
						<table class="table table-striped table-bordered tabbb">
							<tbody style="text-align: center;" id="qy_tbody">
								<tr>
									<th style="text-align: left;">
										<div id="user_select" class="checkbox checkbox-info"></div>
									</th>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
    	</div>
    	<div class="row">
    		<div class="col-sm-10">
    			<button class="btn btn-outline btn-lG" onclick="save()" style="float:right;margin-right:10px;">
					<i class="fa fa-plus-square"></i> 确定创建
				</button>
    		</div>
    	</div>
    </div>
	<script src="../../js/jquery.min.js"></script>
	<script src="../../js/bootstrap.min.js"></script>
	<script src="../../js/content.js"></script>
