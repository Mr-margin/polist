    <!-- 账号创建 -->
    <div class="modal inmodal" id="creat_modal" tabindex="-1"role="dialog" aria-hidden="true" style="width: 55%;margin-left: 22%;">
    	<div class="modal-dialog">
			<div class="modal-content animated bounceInRight">
				<div class="modal-header" style="padding-top:15px;padding-bottom:15px;">
					<button type="button" class="close" data-dismiss="modal">
						<span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
					</button>
					<span style="font-size: 16px;color: rgb(79, 135, 209);padding: 0px 0px 0px 0px;"><h2 id="title_zh">账号创建</h2></span>
				</div>
				
				<div class="modal-body">
					<form class="form-horizontal" id="add_Form">
						<div class="row">
							<div class="row" style="margin-bottom: 10px;">
								<input id="userid" style="display: none;" /> <span
									class="col-sm-3" style="text-align: right;">审核人<code>*</code></span>
								<div class="col-sm-8">
									<input id="tbr_in" name="tbr_in" style="width: 100%;" type="text" required="true" maxlength="4" chinese="true" />
								</div>
							</div>

							<div class="row" style="margin-bottom: 20px;">
								<span class="col-sm-3" style="text-align: right;">所在县<code>*</code></span>
								<div class="col-sm-8">
									<select id="sel_xian" name="sel_xian" name="sel_xian" class="selec_1" required="required" style="height:23px;width:100%" onchange="change_hy(this.value)"></select>
								</div>
							</div>
							
							
<!-- 							<div class="row" style="margin-bottom: 20px;"> -->
<!-- 								<span class="col-sm-3" style="text-align: right;">所在行业<code>*</code></span> -->
<!-- 								<div class="col-sm-8"> -->
<!-- 									<select id="sel_hy" name="sel_hy" name="sel_hy" class="selec_1" required="required"></select> -->
<!-- 								</div> -->
<!-- 							</div> -->
							
							<div class="row" style="margin-bottom: 10px;">
								<span class="col-sm-3" style="text-align: right;">联系电话<code>*</code></span>
								<div class="col-sm-8">
									<input id="lxdh_in" name="lxdh_in" style="width: 100%;" type="text" required="true" mobile="true" />
								</div>
							</div>

							<div class="row" style="margin-bottom: 10px;">
								<span class="col-sm-3" style="text-align: right;">邮箱<code>*</code></span>
								<div class="col-sm-8">
									<input id="yx_in" name="yx_in" style="width: 100%;" type="text" email="true" required="required" />
								</div>
							</div>
							
							<div class="row">
								<span class="col-sm-3" style ="text-align:right;">所在行业<code>*</code></span>
								<div class="col-sm-8 input-group group-select-sm input-group-sm">
									<div  class="qdName_select" style="width:95%;padding-left:15px;">  
										<select id="qdName_select" class="selectpicker" multiple data-live-search="false"  data-live-search-placeholder="Search" data-actions-box="true" style="width:100%;"></select>
									</div>
								</div>
							</div>
							<br>
							<div class="row" style="text-align:center;">
								<button type="button" class="btn btn-outline btn-lB" onclick="chuangjian();" id="add_button"><i class='fa fa-check'></i> 确定创建</button>
								<button type="button" class="btn btn-outline btn-lB"  onclick="xiugai();" id="up_button"><i class='fa fa-check'></i> 确定修改</button>
								<button type="button" class="btn btn-outline btn-lO" onclick="quxiaomtk();"><i class='fa fa-undo'></i> 取消</button>
							</div>
						</div>
					</form>
