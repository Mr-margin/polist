	<style type="text/css">  
	    .anchorBL{  
	       display:none;  
	   }  
	   .alert {
		    margin-bottom: 5px; 
		}
	   .alert-warning {
		    padding: 5px;
		    border-color: #fcf8e3;
		    color:red;
		}
		 .anchorBL{  
	       display:none;  
	   }  
		 #tab_title li{
			 background-color: #dff0d8;
		 }
		 .bootstrap-table .table>tbody>tr>td 
		{
				border: 1px solid #e7e7e7;
				border-left-color: white;
				border-right-color: white;
		}
		.lmtitle {
			border: 1px solid rgb(204, 204, 204); border-image: none; height: 54px; background-color: rgb(255, 255, 255);
		}
		.lmtitle .name {
			padding: 0px 10px 0px 54px; color: rgb(79, 135, 209); line-height: 54px; font-size: 14px; font-weight: bold; float: left; background-repeat: no-repeat;
		}
		
		.lmtitle a.more {
			width: 82px; text-align: center; line-height: 32px; text-decoration: none; float: right; display: block;
		}
		.lmtitle a.more:link {
			margin: 7px; width: 68px; color: rgb(68, 68, 68); line-height: 38px; padding-right: 15px; background-image: url("../images/more_bk2.gif");
		}
		.lmtitle a.more:visited {
			margin: 7px; width: 68px; color: rgb(68, 68, 68); line-height: 38px; padding-right: 15px; background-image: url("../images/more_bk2.gif");
		}
		.lmtitle a.more:hover {
			background-position: bottom; color: rgb(0, 0, 0); background-image: url("../images/more_bk2.gif");
		}
		.lmtitle a.more:active {
			background-position: left bottom; color: rgb(255, 255, 255); background-image: url("../images/more_bk2.gif");
		}
		#navgo {
			margin: 0px 48px 0 0;
			text-align: center;
			float: right;
		}
		
		#navgo ul ul {
			display: none;
		}
		
		#navgo ul li:hover>ul {
			display: block;
		}
		
		#navgo ul {
			padding: 0 0px;
			border-radius: 10px;
			list-style: none;
			position: relative;
			display: inline-table;
		}
		
		#navgo ul:after {
			content: "";
			clear: both;
			display: block;
		}
		
		#navgo ul li {
			float: left;
		}
		
		#navgo ul li:hover {
			background: #72a7de;
		}
		
		#navgo ul li:hover a {
			color: #fff;
		}
		
		#navgo ul li a {
			display: block;
			padding: 10px 10px 13px 10px;
			color: #757575;
			text-decoration: none;
		}
		
		#navgo ul ul {
			background: #5f6975;
			border-radius: 0px;
			padding: 0;
			position: absolute;
			top: 100%;
		}
		
		#navgo ul ul li {
			float: none;
		}
		
		#navgo ul ul li a {
			padding: 15px 10px;
			color: #fff;
		}
		
		#navgo ul ul li a:hover {
			background: #2e63a7;
		}
		
		#navgo ul ul ul {
			position: absolute;
			left: 100%;
			top: 0;
		}
		#tab_body {white-space:nowrap;}
  	</style>
